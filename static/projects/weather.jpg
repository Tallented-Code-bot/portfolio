<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#0f172a"/>

  <!-- Header -->
  <rect x="20" y="20" width="360" height="50" fill="#1e293b" rx="8"/>
  <text x="40" y="42" fill="#f1f5f9" font-size="16" font-weight="bold">Weather Dashboard</text>
  <text x="40" y="58" fill="#94a3b8" font-size="12">San Francisco, CA</text>
  <text x="320" y="50" fill="#f1f5f9" font-size="24" font-weight="bold">72°F</text>

  <!-- Current weather card -->
  <rect x="30" y="90" width="150" height="120" fill="#1e293b" rx="12" stroke="#374151"/>
  <!-- Sun icon -->
  <circle cx="80" cy="130" r="20" fill="#fbbf24"/>
  <line x1="80" y1="100" x2="80" y2="110" stroke="#fbbf24" stroke-width="2"/>
  <line x1="80" y1="150" x2="80" y2="160" stroke="#fbbf24" stroke-width="2"/>
  <line x1="50" y1="130" x2="60" y2="130" stroke="#fbbf24" stroke-width="2"/>
  <line x1="100" y1="130" x2="110" y2="130" stroke="#fbbf24" stroke-width="2"/>
  <text x="80" y="175" text-anchor="middle" fill="#f1f5f9" font-size="12">Sunny</text>
  <text x="80" y="190" text-anchor="middle" fill="#94a3b8" font-size="10">Humidity: 45%</text>
  <text x="80" y="202" text-anchor="middle" fill="#94a3b8" font-size="10">Wind: 8 mph</text>

  <!-- 5-day forecast -->
  <rect x="200" y="90" width="170" height="120" fill="#1e293b" rx="12" stroke="#374151"/>
  <text x="220" y="110" fill="#f1f5f9" font-size="12" font-weight="bold">5-Day Forecast</text>

  <!-- Day 1 -->
  <text x="220" y="130" fill="#94a3b8" font-size="10">Mon</text>
  <circle cx="250" cy="127" r="6" fill="#fbbf24"/>
  <text x="270" y="130" fill="#f1f5f9" font-size="10">75°</text>
  <text x="300" y="130" fill="#94a3b8" font-size="10">62°</text>

  <!-- Day 2 -->
  <text x="220" y="150" fill="#94a3b8" font-size="10">Tue</text>
  <circle cx="250" cy="147" r="6" fill="#6b7280"/>
  <text x="270" y="150" fill="#f1f5f9" font-size="10">68°</text>
  <text x="300" y="150" fill="#94a3b8" font-size="10">55°</text>

  <!-- Day 3 -->
  <text x="220" y="170" fill="#94a3b8" font-size="10">Wed</text>
  <circle cx="250" cy="167" r="6" fill="#3b82f6"/>
  <text x="270" y="170" fill="#f1f5f9" font-size="10">63°</text>
  <text x="300" y="170" fill="#94a3b8" font-size="10">48°</text>

  <!-- Day 4 -->
  <text x="220" y="190" fill="#94a3b8" font-size="10">Thu</text>
  <circle cx="250" cy="187" r="6" fill="#fbbf24"/>
  <text x="270" y="190" fill="#f1f5f9" font-size="10">71°</text>
  <text x="300" y="190" fill="#94a3b8" font-size="10">58°</text>

  <!-- Chart area -->
  <rect x="30" y="230" width="340" height="50" fill="#1e293b" rx="8" stroke="#374151"/>
  <text x="50" y="248" fill="#94a3b8" font-size="10">Temperature Trend</text>
  <!-- Simple line chart -->
  <polyline points="50,265 100,260 150,255 200,258 250,262 300,259 350,256"
            fill="none" stroke="#3b82f6" stroke-width="2"/>
  <circle cx="50" cy="265" r="2" fill="#3b82f6"/>
  <circle cx="100" cy="260" r="2" fill="#3b82f6"/>
  <circle cx="150" cy="255" r="2" fill="#3b82f6"/>
  <circle cx="200" cy="258" r="2" fill="#3b82f6"/>
  <circle cx="250" cy="262" r="2" fill="#3b82f6"/>
  <circle cx="300" cy="259" r="2" fill="#3b82f6"/>
  <circle cx="350" cy="256" r="2" fill="#3b82f6"/>
</svg>
