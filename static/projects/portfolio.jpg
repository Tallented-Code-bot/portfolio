  <text x="75" y="180" text-anchor="middle" fill="#6366f1" font-size="16" font-weight="bold">&lt;/&gt;</text>
  <text x="175" y="180" text-anchor="middle" fill="#06b6d4" font-size="16" font-weight="bold">{ }</text>
  <text x="275" y="180" text-anchor="middle" fill="#10b981" font-size="16" font-weight="bold">&lt;&gt;</text>

  <!-- Footer -->
  <rect x="50" y="235" width="300" height="20" fill="#f1f5f9" rx="0 0 4 4"/>
  <rect x="60" y="242" width="100" height="6" fill="#d1d5db" rx="1"/>
  <rect x="280" y="242" width="60" height="6" fill="#d1d5db" rx="1"/>
