  <rect x="190" y="130" width="60" height="15" fill="#e2e8f0" rx="2"/>
  <rect x="260" y="130" width="60" height="15" fill="#e2e8f0" rx="2"/>

  <!-- Project cards -->
  <rect x="60" y="160" width="80" height="60" fill="#f8fafc" stroke="#e5e7eb" rx="4"/>
  <rect x="65" y="165" width="70" height="30" fill="#e2e8f0" rx="2"/>
  <rect x="65" y="200" width="50" height="8" fill="#cbd5e1" rx="1"/>
  <rect x="65" y="212" width="35" height="6" fill="#9ca3af" rx="1"/>

  <rect x="160" y="160" width="80" height="60" fill="#f8fafc" stroke="#e5e7eb" rx="4"/>
  <rect x="165" y="165" width="70" height="30" fill="#e2e8f0" rx="2"/>
  <rect x="165" y="200" width="55" height="8" fill="#cbd5e1" rx="1"/>
