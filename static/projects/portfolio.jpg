<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#f8fafc"/>

  <!-- Browser window -->
  <rect x="30" y="30" width="340" height="240" fill="#ffffff" stroke="#e2e8f0" stroke-width="2" rx="8"/>

  <!-- Browser bar -->
